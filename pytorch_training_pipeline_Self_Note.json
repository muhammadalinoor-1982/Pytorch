nt Parameters"],"metadata":{"id":"5gn_QAecdLJF"}},{"cell_type":"code","source":["learning_rate = 0.1\n","epochs = 25"],"metadata":{"id":"xkfjyefSXHcv","executionInfo":{"status":"ok","timestamp":1733494601843,"user_tz":-360,"elapsed":10,"user":{"displayName":"Aupu_AI","userId":"12893461163900296745"}}},"execution_count":16,"outputs":[]},{"cell_type":"markdown","source":["### Training Pipeline"],"metadata":{"id":"8IiXlBkKdONn"}},{"cell_type":"code","source":["# create model\n","model = MySimpleNN(X_train_tensor)\n","\n","# define loop\n","for epoch in range(epochs):\n","\n","  # forward pass\n","  y_pred = model.forward(X_train_tensor)\n","\n","  # loss calculate\n","  loss = model.loss_function(y_pred, y_train_tensor)\n","\n","  # backward pass\n","  loss.backward()\n","\n","  # parameters update\n","  with torch.no_grad():\n","    model.weights -= learning_rate * model.weights.grad\n","    model.bias -= learning_rate * model.bias.grad\n","\n","  # zero gradients\n","  model.weights.grad.zero_()\n","  model.bias.grad.zero_()\n","\n","  # print loss in each epoch\n","  print(f'Epoch: {epoch + 1}, Loss: {loss.item()}')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"GxPsNM2_XQev","outputId":"005274e8-12c3-4527-bf0f-4c2764b03aa9","executionInfo":{"status":"ok","timestamp":1733494602177,"user_tz":-360,"elapsed":343,"user":{"displayName":"Aupu_AI","userId":"12893461163900296745"}}},"execution_count":17,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch: 1, Loss: 3.9177294459146452\n","Epoch: 2, Loss: 3.8014313367105377\n","Epoch: 3, Loss: 3.67903486673453\n","Epoch: 4, Loss: 3.552160302974633\n","Epoch: 5, Loss: 3.4155303750569157\n","Epoch: 6, Loss: 3.27500661053229\n","Epoch: 7, Loss: 3.129020437961348\n","Epoch: 8, Loss: 2.9814403036271173\n","Epoch: 9, Loss: 2.8312274287922854\n","Epoch: 10, Loss: 2.6771613200873254\n","Epoch: 11, Loss: 2.523304755920314\n","Epoch: 12, Loss: 2.36602706656088\n","Epoch: 13, Loss: 2.203828573181393\n","